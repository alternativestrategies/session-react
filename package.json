    "express": "^4.18.2",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.6",
    "morgan": "^1.9.1",
    "react-router-dom": "^5.1.2",
    "redux-react-session": "^2.6.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.4"
  }
